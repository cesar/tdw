  <div class="page-header">
    <h1>Current Searches</h1>
  </div>
  <div class="row">
    <div class="col-lg-12">
      <a href="/keywords/new" class="btn btn-success pull-right">Add</a>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-12">
      <table class="table">
        <thead>
          <tr>
            <th>#</th>
            <th>Initiated</th>
            <th>Keywords</th>
            <th>Status</th>
            <th>More</th>
          </tr>
        </thead>
        <tbody>
          {{#each keywords as |keyword|}}
              <tr>
                <td>{{@index}}</td>
                <td>{{keyword.created}}</td>
                <td>{{keyword.firstParameter}} | {{keyword.secondParameter}} | {{keyword.thirdParameter}}</td>
                {{#if keyword.status}}
                  <td>Active</td>
                {{else}}
                  <td>Inactive</td>
                {{/if}}
                <td><a href="/keywords/{{keyword._id}}">View</a></td>
              </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
